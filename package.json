{
  "name": "webpack-simple-starter",
  "version": "1.0.0",
  "description": "A simple webpack starter without framework (Like Vue, React, Angular, etc.)",
  "scripts": {
    "start": "parcel index.html",
    "build": "parcel build index.html --public-url ./; cp -r static dist/static",
    "pub": "gh-pages -d dist -b master"
  },
  "author": "Sinan Mutlu <sinanm17@gmail.com>",
  "license": "MIT",
  "devDependencies": {
    "@types/react": "^16.0.36",
    "@types/react-dom": "^16.0.3",
    "@types/react-router": "^4.0.21",
    "@types/react-router-dom": "^4.2.3",
    "babel-preset-env": "^1.6.1",
    "babel-preset-react": "^6.24.1",
    "node-sass": "^4.7.2",
    "parcel-bundler": "^1.4.1",
    "typescript": "^2.7.1"
  },
  "dependencies": {
    "bootstrap": "^4.0.0",
    "jquery": "^3.2.1",
    "popper.js": "^1.13.0",
    "react": "^16.2",
    "react-dom": "^16.2",
    "react-router": "^4.2.0",
    "react-router-dom": "^4.2.2"
  }
}
